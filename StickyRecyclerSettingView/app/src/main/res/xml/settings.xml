<?xml version="1.0" encoding="UTF-8"?>
<mysettings xmlns:android="http://schemas.android.com/apk/res/android">
  <!--Normal Setting-->
  <myheader
      name="Normal Setting"/>
  <item
      android:id="@+id/id"
      name="ID"
      secondaryTxt="23333"/>
  <item
      android:id="@+id/name"
      name="Name"
      secondaryTxt="Lshare"/>
  <item
      android:id="@+id/sex"
      name="Sex"
      secondaryTxt="male"/>
  <item
      android:id="@+id/city"
      name="City"
      secondaryTxt="Guangzhou"/>
  <item
      android:id="@+id/state"
      name="State"
      secondaryTxt="Coding"/>
  <!--Function Setting-->
  <myheader name="Function Setting"/>
  <item
      android:id="@+id/gift_exchange"
      hightLight="true"
      name="Gift Exchange"
      secondaryTxt="New"/>
  <item name="Live Story Setting"/>
  <!--Share Setting-->
  <myheader name="Share Setting"/>
  <item
      android:id="@+id/qq"
      leftIconRes="R.drawable.ic_qzone"
      name="QQ"
      showRightIcon="false"
      showSwitch="off"
      />
  <item
      android:id="@+id/we_chat"
      leftIconRes="R.drawable.ic_wechat"
      name="WeChat"
      showRightIcon="false"
      showSwitch="on"
      />
  <item
      android:id="@+id/sina"
      leftIconRes="R.drawable.ic_sina"
      name="Sina"
      showRightIcon="false"
      showSwitch="on"
      />
  <!--Other Setting-->
  <myheader name="Other Setting"/>
  <item
      android:id="@+id/feedback"
      name="Feedback"/>
  <item
      android:id="@+id/aboutus"
      name="About Us"/>
  <item
      android:id="@+id/update"
      name="Check Update"/>
  <item
      android:id="@+id/exit"
      name="Exit"/>
  <!--Other Setting-->
  <myheader name="Other Setting"/>
  <item
      android:id="@+id/feedback2"
      name="Feedback"/>
  <item
      android:id="@+id/aboutus2"
      name="About Us"/>
  <item
      android:id="@+id/update2"
      name="Check Update"/>
  <item
      android:id="@+id/exit2"
      name="Exit"/>
</mysettings>